{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nsis-reference",
  "title": "NSIS Reference",
  "description": "Search NSIS references",
  "icon": "command-icon.png",
  "author": "idleberg",
  "contributors": [
    "xmok"
  ],
  "categories": [
