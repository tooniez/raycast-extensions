  ],
  "dependencies": {
    "@nsis/docs": "^0.4.1",
    "@raycast/api": "^1.104.5",
    "@sindresorhus/slugify": "^3.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "^20.8.10",
    "@types/react": "^19.2.13",
    "eslint": "^9.22.0",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS"
